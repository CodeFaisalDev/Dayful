{
  "expo": {
    "name": "Dayful",
    "slug": "dayful",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "dayful",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#a193d4"
    },
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "newArchEnabled": true,
      "package": "com.faisal2001.dayful"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-community/datetimepicker",
      "expo-audio",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-9134194923528949~4589569272",
          "iosAppId": "ca-app-pub-9134194923528949~4589569272"
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "894e6824-376b-4f92-9551-28467eed1ba4"
      }
    }
  }
}